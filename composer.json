{
    "name": "giannels/test-result-laravel",
    "license": "MIT",
    "description": {
        "Package untuk export hasil test Laravel ke PDF",
        "README" : "https://github.com/giannels/test-result-laravel"
    },
    
    
    "autoload": {
    "psr-4": {
        "giannels\\TestResultLaravel\\": "src/"
        }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "giannels\\TestResultLaravel\\TestResultServiceProvider"
            ]
        }
    },

    "require": {
        "barryvdh/laravel-dompdf": "^2.0",
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    }
}
